def draw(matrix, size, colors, dico, marginX=0, marginY=0):
    global screen
    for x in range(len(matrix[0])):
        for y in range(len(matrix)):
            if matrix[y][x] in colors:
                if len(colors[matrix[y][x]]) == 2:
                    if dico[str(colors[matrix[y][x]][1])] == "False":
                        if matrix == body:
                            pygame.draw.rect(screen, (255,255,255), (size*(x+marginX), size*(y+marginY), size, size))
                        else:
                            pygame.draw.rect(screen, (229,57,53), (size*(x+marginX), size*(y+marginY), size, size))
                    else:
                        pygame.draw.rect(screen, colors[matrix[y][x]][0], (size*(x+marginX), size*(y+marginY), size, size))
                else:
                    pygame.draw.rect(screen, colors[matrix[y][x]], (size*(x+marginX), size*(y+marginY), size, size))

    if innit == False: draw_settings_icon(settings_icon, size)
    pygame.display.update()


def get_exo():
    listexo = []
    muscle_names = set(bool_dico_msucle.keys())

    with open("Projet NSI.txt", "r") as f:
        data = f.read()
    for exo in data.split('\n'):
        exo_data = exo.split(';')
        
        if exo_data[1]:  # Check if the exercise name is not empty
            if any(str(muscle) in str(exo_data[2]) and bool_dico_msucle[muscle] == "True" for muscle in muscle_names):
                if "Pas de matriel" in exo_data[5] and aucun or "Matrielle achetable" in exo_data[5] and MaterielleAchetable or "A faire en salle" in exo_data[5] and EnSalle:
                    listexo.append((exo_data[1], exo_data[2], exo_data[3]))

    return listexo


def draw_settings_icon(settings_icon, size):
    global screen
    for x in range(len(settings_icon[0])):
        for y in range(len(settings_icon)):
            if settings_icon[y][x] == 0:
                pygame.draw.rect(screen, (38,50,56), ((x+76)*size, (y+3)*size, size, size))

    pygame.display.update()


def main():
    screen.fill(background_colour)
    if innit == True:
        draw(home_screen, size, home_screen_colors, None, 20, 34)
        if sound == True: start_sound.play()
        sleep(0.5)
        draw(instructions, size, instructions_colors, None, 20, 41)
    else:
        if settings == True:
            draw(menu, size, menu_colors, None, 20, 31)
        else:
            draw(body, size, body_colors, bool_dico_msucle)

    pygame.display.flip()


"""
   _____         .__        
  /     \ _____  |__| ____  
 /  \ /  \\__  \ |  |/    \ 
/    Y    \/ __ \|  |   |  \
\____|__  (____  /__|___|  /
        \/     \/        \/ 
"""
main()
while running:
    for event in pygame.event.get():
        try:
            if event.type == pygame.MOUSEBUTTONDOWN:
                Xpos, Ypos = pygame.mouse.get_pos()
                Xcoo, Ycoo = round(Xpos/size), round(Ypos/size)
                if innit == True:
                    innit = False
                    draw(body, size, body_colors, bool_dico_msucle)

                if Xcoo >= 76 and Xcoo <= 82 and Ycoo >= 2 and Ycoo <= 7:
                    if settings == False:
                        settings = True
                    else:
                        settings = False
                    Materiel_menu_display = False
                    main()
                
                if settings == True and Xcoo >= 20 and Xcoo <= 68 and Ycoo >= 30 and Ycoo <= 41:
                    settings = False
                    Materiel_menu_display = True
                    screen.fill(background_colour)
                    draw(materiel_menu, size, materiel_menu_colors, bool_dico_matos)

                if settings == False and innit == False and Materiel_menu_display == False:
                    muscle_click = bool_dico_msucle[str(body_colors[body[round(Ypos/size)][round(Xpos/size)]][1])]
                    if muscle_click == "True":
                        bool_dico_msucle[str(body_colors[body[round(Ypos/size)][round(Xpos/size)]][1])] = "False"
                    else:
                        bool_dico_msucle[str(body_colors[body[round(Ypos/size)][round(Xpos/size)]][1])] = "True"
                        if sound == True:
                            select_sound.play()
                        exos = get_exo()
                        if exos != []:
                            print(exos)
                        
                    draw(body, size, body_colors, bool_dico_msucle)
                    
                elif settings == False and innit == False and Materiel_menu_display == True:
                    materiel_click = bool_dico_matos[str(materiel_menu_colors[materiel_menu[round(Ypos/size)][round(Xpos/size)]][1])]
                    if materiel_click == "True":
                        bool_dico_matos[str(materiel_menu_colors[materiel_menu[round(Ypos/size)][round(Xpos/size)]][1])] = "False"
                    else:
                        bool_dico_matos[str(materiel_menu_colors[materiel_menu[round(Ypos/size)][round(Xpos/size)]][1])] = "True"
                        if sound == True:
                            select_sound.play()
                    draw(materiel_menu, size, materiel_menu_colors, bool_dico_matos)

            if event.type == pygame.QUIT:
                running = False
        except:
            pass
